{
  "version": "0.6",
  "tasks": [
    {
      "name": "Example Task",
      "requests": [
        {
          "url": "https://httpbin.org/get",
          "method": "GET",
          "headers": {
            "Content-Type": "application/json",
            "User-Agent": "httpx/0.28.0"
          }
        },
        {
          "url": "https://httpbin.org/post",
          "method": "POST",
          "data": "value1=data1&value2=data2"
        }
      ],
      "rules": [
      ],
      "policy": {
        "reuse_connections": true,
        "order": "random",
        "schedule": {
          "start": 0,
          "end": 30
        },
        "limits": {
          "rps": 7.5,
          "coroutines": 16
        },
        "timeouts": {
          "connect": 5.0,
          "read": 10.0,
          "write": 10.0
        }
      },
      "prefabs": {
        "override_hosts": {
          "example.com": "1.2.3.4",
          "www.example.com": "1.2.3.4"
        },
        "default_headers": {
          "Accept-Encoding": "gzip, deflate, br, zstd",
          "Accept-Language": "en"
        }
      },
      "proxies": [
        null
      ]
    }
  ],
  "server": {
    "port": 5000,
    "distributing": {
      "task_order": "random"
    }
  },
  "client": {
    "server_url": "http://127.0.0.1:5000",
    "report": {
      "live_report_interval": 30
    }
  }
}
